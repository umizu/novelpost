{
  "Database": {
    "ConnectionString": "Data source=./novelpost.db"
  },
  "JwtSettings": {
    "Issuer": "novelpost",
    "Audience": "novelpost",
    "ExpiryMinutes": 60,
    "Secret": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}
